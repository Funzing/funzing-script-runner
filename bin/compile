echo "SOURCE_DIR $SOURCE_DIR"
ls -lr "$SOURCE_DIR"
node -v
ruby -v
run_build() {
    ls -l "/tmp/test/my dir"
  cd "$BUILD_DIR"
  npm i -g ionic cordova npm @angular/cli
  ./buildBrowserXYZ.sh
}
